{
  "name": "soup-build",
  "displayName": "Soup Build",
  "description": "Soup Build Integrations",
  "repository": {
    "type": "git",
    "url": "https://github.com/SoupBuild/SoupVSCode.git"
},
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sml",
        "aliases": [
          "SML",
          "sml"
        ],
        "extensions": [
          ".sml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sml",
        "scopeName": "source.sml",
        "path": "./syntaxes/sml.tmLanguage.json"
      }
    ]
  }
}
